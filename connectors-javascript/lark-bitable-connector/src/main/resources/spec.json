{
  "properties": {
    "name": "Feishu-Bitable",
    "icon": "icon/bitable.png",
    "doc" : "${doc}",
    "id": "feishu-bitable",
    "tags": ["SaaS"]
  },
  "configOptions": {
    "capabilities": [
      {
        "id": "dml_insert_policy",
        "alternatives": [
          "update_on_exists",
          "ignore_on_exists",
          "just_insert"
        ]
      },
      {
        "id": "dml_update_policy",
        "alternatives": [
          "ignore_on_nonexists",
          "insert_on_nonexists"
        ]
      }
    ],
    "pdkExpansion": [],
    "node": {
      "type": "object",
      "properties": {
        "queryApiDelay": {
          "required": true,
          "type": "string",
          "title": "${QueryDelayMillSeconds}",
          "default": 2000,
          "x-index": 10,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "x-decorator-props": {
            "tooltip": "${QueryDelayMillSecondsTip}"
          },
          "x-component-props": {
            "min": 0,
            "max": 10000
          }
        },
        "writeApiDelay": {
          "required": true,
          "type": "string",
          "title": "${WriteDelayMillSeconds}",
          "default": 0,
          "x-index": 20,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "x-decorator-props": {
            "tooltip": "${WriteDelayMillSecondsTip}"
          },
          "x-component-props": {
            "min": 0,
            "max": 10000
          }
        }
      }
    },
    "connection": {
      "type": "object",
      "properties": {
        "app_id": {
          "type": "String",
          "title": "${app_id}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 10,
          "required": true
        },
        "app_secret": {
          "type": "String",
          "title": "${app_secret}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 20,
          "required": true
        },
        "app_token": {
          "type": "String",
          "title": "${app_token}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 30,
          "required": true
        },
        "table_id": {
          "type": "String",
          "title": "${table_id}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 40,
          "required": true
        }
      }
    }
  },
  "messages": {
    "default": "en_US",
