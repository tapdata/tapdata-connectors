{
  "properties": {
    "name": "QuickAPI",
    "icon": "icons/quickapi.png",
    "doc" : "${doc}",
    "id": "quickapi",
    "tags": ["SaaS"]
  },
  "configOptions": {
    "pdkExpansion": [],
    "connection": {
      "type": "object",
      "properties": {
        "apiType": {
          "type": "String",
          "default": "POST_MAN",
          "title": "${apiType}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "enum": [
            {
              "label": "PostMan",
              "value": "POST_MAN"
            }
          ],
          "x-index": 10,
          "required": false,
          "x-display": "hidden"
        },
        "jsonTxt": {
          "type": "String",
          "title": "${jsonTxt}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-component-props": {
            "type": "textarea"
          },
          "x-index": 20,
          "required": true
        },
        "expireStatus": {
          "title": "${expireStatus}",
          "type": "String",
          "required": false,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-component-props": {
            "type": "textarea"
          },
          "x-index": 30,
          "x-reactions": [
            {
              "dependencies": ["jsonTxt"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "tokenParams": {
          "type": "String",
          "title": "${tokenParams}",
          "x-decorator": "FormItem",
          "x-index": 40,
          "x-component": "Input",
          "x-component-props": {
            "type": "textarea"
          },
          "x-reactions": [
            {
              "dependencies": ["expireStatus"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ],
          "required": false
        },
        "autoSchema": {
          "type": "boolean",
          "default": true,
          "title": "${autoSchema}",
          "x-decorator-props": {
            "colon": false,
            "tooltip": "${autoSchema}"
          },
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 50
        },
        "sampleData": {
          "type": "String",
          "title": "${sampleData}",
          "default": "{\"table_name_1\":{},\"table_name_2\":{}}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-component-props": {
            "type": "textarea"
          },
          "x-index": 60,
          "required": true,
          "x-reactions": {
            "dependencies": ["autoSchema"],
            "fulfill": {
              "state": {
                "display": "{{$deps[0] ? \"hidden\" : \"visible\"}}"
              }
            }
          }
        }
      }
    },
    "node": {
      "type": "object",
      "properties": {
      }
    }
  },
  "messages": {
    "default": "en_US",
    "en_US": {
      "doc": "docs/quickapi_en_US.md",
      "apiType": "Commissioning platform",
      "jsonTxt": "Postman Collection in JSON",
      "expireStatus": "Expiration Status Desc",
      "tokenParams": "Token parameter specification",
      "hookButton": "Generate Service URL",
      "autoSchema": "Automatically retrieve table structure",
      "sampleData": "Sample Data（JSON）"
    },
    "zh_CN": {
      "doc": "docs/quickapi_zh_CN.md",
      "apiType": "调试平台",
      "jsonTxt": "PostMan导出的API JSON",
      "expireStatus": "过期状态描述",
      "tokenParams": "Token 参数指定",
      "hookButton": "生成服务URL",
      "autoSchema": "自动获取表结构",
      "sampleData": "样本数据（JSON）"
    },
    "zh_TW": {
      "doc": "docs/quickapi_zh_TW.md",
      "apiType": "調試平臺",
      "jsonTxt": "PostMan匯出的API JSON",
      "expireStatus": "過期狀態描述",
      "tokenParams": "Token 參數指定",
      "hookButton": "生成服務URL",
      "autoSchema": "自動獲取錶結構",
      "sampleData": "樣本數據（JSON）"
    }
  },
  "dataTypes": {
    "Object": {
      "to": "TapMap"
    },
    "Number": {
      "precision": [
        1,
        1000
      ],
      "scale": [
        0,
        1000
      ],
      "fixed": true,
      "preferPrecision": 20,
      "preferScale": 8,
      "priority": 1,
      "to": "TapNumber"
    },
    "Date": {
      "range": [
        "1000-01-01",
        "9999-12-31"
      ],
      "pattern": "yyyy-MM-dd",
      "fraction": 0,
      "defaultFraction": 0,
      "withTimeZone": false,
      "priority": 2,
      "to": "TapDate"
    },
    "DateTime": {
      "range": [
        "1000-01-01 00:00:00.000",
        "9999-12-31 23:59:59.999"
      ],
      "pattern": "yyyy-MM-dd hh:mm:ss.sss",
      "fraction": [
        0,
        9
      ],
      "defaultFraction": 3,
      "withTimeZone": false,
      "priority": 2,
      "to": "TapDateTime"
    },
    "Long": {
      "bit": 64,
      "priority": 10,
      "value": [
        -9223372036854775808,
        9223372036854775807
      ],
      "scale": 0,
      "to": "TapNumber"
    },
    "Boolean": {
      "to": "TapBoolean"
    },
    "String": {
      "byte": 1024,
      "priority": 1,
      "defaultByte": 512,
      "preferByte": 1024,
      "to": "TapString"
    },
    "Array": {
      "to": "TapArray"
    },
    "Float": {
      "to": "TapNumber",
      "bit": 64,
      "scale": [
        0,
        6
      ],
      "fixed": false
    }
  }
}
