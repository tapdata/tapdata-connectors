  "messages": {
    "default": "en_US",
    "en_US": {
      "doc": "docs/postgres_en_US.md",
      "deploymentMode": "Deployment mode",
      "standalone": "Single machine deployment",
      "master-slave": "Master-Slave Architecture",
      "master-slaveAddress" : "Server address",
      "master-slaveAddressTip" : "The address port group that the master-slave architecture needs to fill in, the address port of the master database needs to be filled in the first line, for example: 192.168.1.200:3306, 192.168.1.201:3307",
      "Address" : "Please enter the server address",
      "serverPort": "Server port",
      "prompt": "Add",
      "host": "Host",
      "hostTip": "The address of the database, it can be an IP address or a domain name, for example: 192.168.1.200",
      "port": "Port",
      "portTip": "The port number of the database, the default port of Postgres is 5432",
      "database" : "database",
      "databaseTip": "database name, you can query all databases through pg_database",
      "schema" : "schema",
      "schemaTip": "schema name, you can query all schemas through pg_namespace",
      "extParams" : "extParams",
      "extParamsTip": "Additional connection parameters in URI, you can write according to personalized scenarios \nfor example: ?binaryTransfer=false&forceBinary=false&reWriteBatchedInserts=true",
      "user" : "user",
      "password" : "password",
      "logPluginName" : "logPluginName",
      "logPluginNameTip": "The higher version PG comes with the PGOUTPUT plugin, but the before data in the logs is not complete. \nThe author recommends using WAL2JSON, and the installation method can be found in the MD documentation",
      "pgtoHost": "Pgto Server Host",
      "pgtoPort": "Pgto Server Port",
      "partPublication": "Part publication",
      "partPublicationTooltip": "Turn on PGOUTPUT plugin to create corresponding publication according to different collection tables. The advantage is that there is no limit to the update and delete of no primary key table without collection requirements at the replica level. The disadvantage is that too many tasks will affect the database performance, and create publication permission is required",
      "globalPublicationName": "Global publication name",
      "globalPublicationNameTooltip": "When partial publication is turned off, a global publication is required. The default name is dbz_publication. If you need to customize the settings of the publication, it is recommended that customers create the publication themselves according to the name. The synchronization account does not need additional permissions",
