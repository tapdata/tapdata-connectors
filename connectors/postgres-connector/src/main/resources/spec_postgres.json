              "title": "${allowReplication}",
              "default": true,
              "x-index": 100,
              "x-decorator": "FormItem",
              "x-component": "Switch",
              "x-decorator-props": {
                "tooltip": "${allowReplicationTooltip}"
              }
            },
            "defaultWalLogSize": {
              "type": "string",
              "title": "${defaultWalLogSize}",
              "default": 102400,
              "x-decorator": "FormItem",
              "x-component": "InputNumber",
              "x-decorator-props": {
                "tooltip": "${defaultWalLogSizeTip}"
              },
              "x-index": 110
            }
          }
        }
      }
